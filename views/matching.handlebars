  <div class="container-fluid matches">
    <div class="row">
      <div class="col-lg-4">
        <a href="/">
          <img class="logout" src="assets/img/logo-exit.png">
        </a>        
      </div>
      <div class="col-lg-4">
        <img class="logo" src="assets/img/logo.png">
      </div>
      <div class="col-lg-4">
        <a href="/my-matches">
          <img class="my-matches-button" src="assets/img/my-matches-icon.png">
        </a>
        
      </div>
    </div>
    <div class="row displayHouses">
 
      <div class="col-lg-12">
    
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
        {{> listing-block}}
          </div>
        </div>
      </div>

    </div>
  <div class="row decisions">
      <div class="col-md-6 decisionContainer">
        <a class="decisionButton" data-decision="no" role="button" data-slide="next">
          <img class="swipeControls" src="assets/img/no-02.png">
        </a>
      </div>
      <div class="col-md-6 decisionContainer">
        <a class="decisionButton" data-decision="yes" role="button" data-slide="next">
          <img class="swipeControls" src="assets/img/yes-01.png">
        </a>
      </div>
  </div>
</div>
<script type="text/javascript" src="js/matching.js"></script>