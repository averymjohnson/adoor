  <div class="container-fluid matches">
    <div class="row">
      <div class="col-lg-4">
        <a href="/">
          <img class="logout" src="assets/img/logo-exit.png">
        </a>        
      </div>
      <div class="col-lg-4">
        <img class="logo" src="assets/img/logo.png">
      </div>
      <div class="col-lg-4">
        <a href="/my-matches">
          <img class="my-matches-button" src="assets/img/my-matches-icon.png">
        </a>
        
      </div>
    </div>
    <div class="row displayHouses">
 
      <div class="col-lg-12">
    
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
        {{> listing-block}}
          </div>
        </div>
      </div>

    </div>
  <div class="row decisions">
      <div class="col-md-6 decisionContainer">
        <a class="decisionButton" data-decision="no" role="button" data-slide="next">
          <img class="swipeControls" src="assets/img/no-02.png">
        </a>
      </div>
      <div class="col-md-6 decisionContainer">
        <a class="decisionButton" data-decision="yes" role="button" data-slide="next">
          <img class="swipeControls" src="assets/img/yes-01.png">
        </a>
      </div>
  </div>
</div>

<!--Matching modal -->
<div class="modal fade" id="matchModal" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h2>It's A Match!</h2>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <h5 class="modal-title" id="matchAddress">Match Address</h5>
        <div id="map"></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>

<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBlT2Tk3-9ijc0QIJ2u6YXFESzKMJ5Myrk&callback=initMap"></script>
<script type="text/javascript" src="js/matching.js"></script>